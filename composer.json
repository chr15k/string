{
    "name": "chr15k/string",
    "description": "String helpers package",
    "type": "library",
    "authors": [
        {
            "name": "Chris Keller",
            "email": "christhecoder85@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "doctrine/inflector": "~1.1",
        "ramsey/uuid": "^4.0",
        "voku/portable-ascii": "^1.5"
    },
    "autoload": {
        "psr-4": {
            "ChrisTelKel\\String\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "ChrisTelKel\\String\\Test\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "require-dev": {
        "phpunit/phpunit": "^9.2"
    }
}
