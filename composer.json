{
    "name": "christelkel/string",
    "description": "String helpers package",
    "type": "library",
    "authors": [
        {
            "name": "Chris",
            "email": "christhecoder85@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "doctrine/inflector": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "ChrisTelKel\\Str\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "ChrisTelKel\\Str\\Test\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "require-dev": {
        "phpunit/phpunit": "^9.2"
    }
}
